{
  "name": "tryout-build-blocksuite",
  "version": "0.0.0-beta.0",
  "description": "Build helper for Blocksuite packages sourced from the AFFiNE monorepo.",
  "type": "module",
  "private": true,
  "scripts": {
    "build-blocksuite": "vite-node scripts/build-blocksuite.ts",
    "release-blocksuite": "vite-node scripts/build-blocksuite.ts release",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "build": "tsc --noEmit",
    "check": "pnpm run format && pnpm run typecheck && pnpm run test && pnpm run build"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "@actions/artifact": "^4.0.0",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "@octokit/rest": "^21.0.2",
    "zx": "^8.1.5"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "vite-node": "^2.1.4",
    "vitest": "^2.1.4",
    "@types/fs-extra": "^11.0.4"
  }
}
